{%- liquid
  case color
    when 'Darkest'
      echo 'text-p-darkest'
    when 'Dark'
      echo 'text-p-dark'
    when 'Neutral'
      echo 'text-p-neutral'
    when 'Light'
      echo 'text-p-light'
    when 'Lightest'
      echo 'text-p-lightest'
    when 'Foreground'
      echo 'text-t-foreground'
    when 'Foreground Secondary'
      echo 'text-t-foreground-secondary'
    when 'Background'
      echo 'text-t-background'
    when 'Background Secondary'
      echo 'text-t-background-secondary'
    when 'Brand Primary'
      echo 'text-t-brand-primary'
    when 'Brand Secondary'
      echo 'text-t-brand-secondary'
    when 'Brand Primary Contrast'
      echo 'text-t-brand-primary-contrast'
    when 'Brand Secondary Contrast'
      echo 'text-t-brand-secondary-contrast'
  endcase
-%}
